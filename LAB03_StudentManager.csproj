<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="FormResult.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="FormResult.designer.cs">
      <DependentUpon>FormResult.cs</DependentUpon>
    </Compile>
    <Compile Update="frmStudent.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="frmStudent.designer.cs">
      <DependentUpon>frmStudent.cs</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="FormResult.resx">
      <DependentUpon>FormResult.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

</Project>